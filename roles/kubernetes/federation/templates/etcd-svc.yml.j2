apiVersion: v1
kind: Service
metadata:
  name: coredns-etcd
  namespace: {{ system_namespace }}
  labels:
    k8s-app: coredns-etcd
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "coredns-etcd"
spec:
  selector:
    k8s-app: coredns-etcd
  ports:
  - name: client
    port: 2379
    protocol: TCP
    targetPort: 2379
  - name: server
    port: 2380
    protocol: TCP
    targetPort: 2380
