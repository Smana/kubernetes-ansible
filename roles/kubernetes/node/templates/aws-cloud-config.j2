[Global]
{% if kube_version | version_compare('v1.6', '>=') %}
zone={{ aws_zone|d("") }}
vpc={{ aws_vpc|d("") }}
subnetId={{ aws_subnet_id|d("") }}
routeTableId={{ aws_route_table_id|d("") }}
kubernetesClusterTag={{ aws_kubernetes_cluster_tag|d("") }}
kubernetesClusterId={{ aws_kubernetes_cluster_id|d("") }}
disableSecurityGroupIngress={{ "true" if aws_disable_security_group_ingress|d(False) else "false" }}
disableStrictZoneCheck={{ "true" if aws_disable_strict_zone_check|d(False) else "false" }}
{% if kube_version | version_compare('v1.7', '>=') %}
elbSecurityGroup={{ aws_elb_security_group|d("") }}
{% endif %}
{% endif %}