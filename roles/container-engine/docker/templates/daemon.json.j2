{
    "data-root": "{{ docker_data_root }}",
    "exec-opts": [
        "native.cgroupdriver=cgroupfs"
    ],
    "iptables": "{{ docker_iptables | bool }}"
    "log-driver": "json-file",
    "log-opts": {
        "max-size": "100m",
    },
    "storage-driver": "{{ docker_storage_driver }}",
    "storage-opts": [
        "overlay2.override_kernel_check=true"
    ]
}
